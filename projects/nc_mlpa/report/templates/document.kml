<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2" xmlns:atom="http://www.w3.org/2005/Atom" xmlns:mm="http://marinemap.org" xmlns:gx="http://www.google.com/kml/ext/2.2">
<Document>
	<name>{% block docname %}{{document.name}}{% endblock %}</name>
	<open>1</open>
	<visibility>1</visibility>
	<styleUrl>#radioFolder</styleUrl>
	{% block style %}
		<Style id="spacingStyles">
	    	<LabelStyle>
		      <color>7fffaaff</color>
		      <scale>1.5</scale>
		    </LabelStyle>
		    <LineStyle>
		      <color>ff00ffff</color>
		      <width>5</width>
		    </LineStyle>
		    <PolyStyle>
		      <color>7f7faaaa</color>
			  <fill>0</fill>
			  <outline>1</outline>
		    </PolyStyle>
		    <ListStyle>
			  <listItemType>check</listItemType>
			</ListStyle>
	    </Style>
		<Style id="too_far">
			<IconStyle>
			  <scale>0</scale>
			</IconStyle>
			<LabelStyle>
		      <scale>0</scale>
		    </LabelStyle>
			<LineStyle>
				<width>5</width>
				<color>7f1400FF</color>
			</LineStyle>
		</Style>
		<Style id="close_enough">
			<IconStyle>
			  <scale>0</scale>
			</IconStyle>
			<LabelStyle>
		      <scale>0</scale>
		    </LabelStyle>
			<LineStyle>
				<width>5</width>
				<color>ff14F000</color>
			</LineStyle>
		</Style>
		<Style id="too_far_labels_on">
			<IconStyle>
			  <scale>0</scale>
			</IconStyle>
			<LabelStyle>
		      <scale>1</scale>
		    </LabelStyle>
			<LineStyle>
				<width>8</width>
				<color>7f1400FF</color>
			</LineStyle>
		</Style>
		<Style id="close_enough_labels_on">
			<IconStyle>
			  <scale>0</scale>
			</IconStyle>
			<LabelStyle>
		      <scale>1</scale>
		    </LabelStyle>
			<LineStyle>
				<width>8</width>
				<color>ff14F000</color>
			</LineStyle>
		</Style>
		  <Style id="radioFolder">	
			<ListStyle>
			  <listItemType>radioFolder</listItemType>
			</ListStyle>
			<visibility>0</visibility>
		  </Style>
		<StyleMap id="too_far_map">
			<Pair>
				<key>normal</key>
				<styleUrl>#too_far</styleUrl>
			</Pair>
			<Pair>
				<key>highlight</key>
				<styleUrl>#too_far_labels_on</styleUrl>
			</Pair>
		</StyleMap>
		<StyleMap id="close_enough_map">
			<Pair>
				<key>normal</key>
				<styleUrl>#close_enough</styleUrl>
			</Pair>
			<Pair>
				<key>highlight</key>
				<styleUrl>#close_enough_labels_on</styleUrl>
			</Pair>
		</StyleMap>
	{% endblock %}
	{% block folders_and_placemarks %}
		<Folder>
			<Style>
			<ListStyle>
			  <listItemType>radioFolder</listItemType>
			</ListStyle>
			</Style>
			<open>1</open>
			{% for folder in document.folders %}
				{% include "radio_folder.kml" %}
			{% endfor %}
		</Folder>
	{% endblock %}
</Document>
</kml>